CREATE TABLE objects (
    object_id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    description TEXT,
    length INTEGER,
    width INTEGER,
    height INTEGER,
    location_name TEXT
);

getAllObjects:
SELECT * FROM objects;

getObjectById:
SELECT * FROM objects WHERE object_id = ?;

insertObject:
INSERT INTO objects(name, description, length, width, height, location_name)
VALUES (?, ?, ?, ?, ?, ?);
